{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Services"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "TitleInmenu": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "Images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "ShortDescription": {
      "type": "text"
    },
    "Description": {
      "type": "richtext"
    },
    "SEO": {
      "type": "dynamiczone",
      "components": [
        "shared.seo"
      ]
    },
    "Menu": {
      "type": "dynamiczone",
      "components": [
        "shared.menus-in-events"
      ]
    },
    "File": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "files"
      ]
    },
    "GaleryOfMedia": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "order": {
      "type": "integer",
      "default": 0
    }
  }
}
